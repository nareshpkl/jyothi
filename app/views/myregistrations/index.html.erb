<p id="notice"><%= notice %></p>

<h1>Listing Myregistrations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Cell</th>
      <th>Gender</th>
      <th>Category</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myregistrations.each do |myregistration| %>
      <tr>
        <td><%= myregistration.name %></td>
        <td><%= myregistration.email %></td>
        <td><%= myregistration.cell %></td>
        <td><%= myregistration.gender %></td>
        <td><%= myregistration.category %></td>
        <td><%= myregistration.subject %></td>
        <td><%= link_to 'Show', myregistration %></td>
        <td><%= link_to 'Edit', edit_myregistration_path(myregistration) %></td>
        <td><%= link_to 'Destroy', myregistration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myregistration', new_myregistration_path %>
